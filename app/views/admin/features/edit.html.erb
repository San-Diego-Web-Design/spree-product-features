<%= error_messages_for :feature, :header => false %>

<% form_for :feature, @feature, :url => admin_feature_url(@feature), :html => {:multipart => true, :method => :put} do |f| -%>
  <dl>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <dt></dt>
    <dd><%= f.submit 'Update' %></dd>
  </dl>
<% end -%>

